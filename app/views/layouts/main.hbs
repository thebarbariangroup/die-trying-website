<body>
  <div id="warningIE" class="browser-warning">
    <div class="browser-warning_content">We support the Earth, not Internet Explorer</div>
  </div>
  <script>
    function isIE () {
      return window.navigator.userAgent.match(/(MSIE|Trident)/);
    }
    function dontEven () {
      const warningEl = document.querySelector('#warningIE');

      document.body.setAttribute('style', 'overflow: hidden');
      warningEl.setAttribute('style', 'display: block; z-index: 999');
    }

    if (isIE()) dontEven();
  </script>
  <div class="page-container {{classes}}">
    {{> 'global/nav/head-nav'}}
    <main class="content-container">
      {{#block 'body'}}{{/block}}
    </main>
    {{> 'global/footer/footer' }}
  </div>
  <script src="/assets/scripts/index.js"></script>
</body>
</html>
